package entities;

public class ContaBancaria {
	private int number;
	private String holder;
	private double balance;	
	
	public ContaBancaria(int number, String holder, double balance ) {
		this.number = number;
		this.holder = holder;
		this.balance = balance;
	}
	
	public ContaBancaria(int number, String holder) {
		this.number = number;
		this.holder = holder;		
	}
	
	public int getNumber() {
		return number;
	}
	
	public String getHolder() {
		return holder;
	}
	
	public void setHolder() {
		this.holder = holder;
	}
	
	public double getBalance() {
		return balance;
	}
	
	public void setInitialDeposit() {
		this.balance = balance;
	}
	
	
	
}
